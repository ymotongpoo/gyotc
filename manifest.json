{
    "name": "gyotc",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "an extension to redirect to web gyotaku when opened web site is 404.",
    "background": {
        "scripts": ["background.js"],
        "persistenet": false
    },
    "browser_action": {
        "default_icon": {
            "16": "img/normal16.png",
            "24": "img/normal24.png",
            "32": "img/normal32.png",
            "64": "img/normal64.png"
        },
        "default_title": "gyotc"
    },  
    "permissions": [
        "tabs",
        "webRequest",
        "webNavigation",
        "<all_urls>"
    ]
}